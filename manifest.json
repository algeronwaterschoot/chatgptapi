{
  "manifest_version": 3,
  "name": "ChatGPTApi",
  "version": "1.0",
  "description": "Use ChatGPT as an API.",
  "permissions": ["activeTab", "storage", "scripting", "cookies"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
